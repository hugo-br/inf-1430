{
  "name": "rubydor",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve:frontend": "vue-cli-service serve frontend/main.ts --port 8000",
    "serve:backend": "nodemon --config nodemon.json backend/app.ts",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "vue lint": "vue-cli-service",
    "lint": "lint eslint src/**/*.{js,ts}",
    "lint:fix": "eslint src/**/*.{js,ts}  --fix",
    "ci:prettier": "prettier --check ./**/*.{js,ts,json,vue,css,less,html}",
    "fix:format": "prettier --write ./**/*.{js,ts,json,vue,css,less,html}"
  },
  "dependencies": {
    "@types/compression": "^1.7.1",
    "@types/express-serve-static-core": "^4.17.24",
    "@types/morgan": "^1.9.3",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongoose": "^5.12.7",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.22",
    "nodemon": "^2.0.12",
    "pino": "^6.11.3",
    "pino-pretty": "^4.7.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "vuex-class": "^0.3.2",
    "vuex-class-modules": "^1.3.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/body-parser": "^1.19.0",
    "@types/config": "^0.0.38",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^24.0.19",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.168",
    "@types/mongoose": "^5.10.5",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^15.0.1",
    "@types/pino": "^6.3.8",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "tsc-alias": "^1.3.8",
    "typescript": "~4.1.5",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest/presets/typescript-and-babel"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "delay": 2500
  },
  "_moduleAliases": {
    "@root": ".",
    "@frontend": "frontend/",
    "@components": "frontend/components/",
    "@backend": "backend/",
    "@controller": "backend/controller/",
    "@middleware": "backend/middleware/",
    "@model": "backend/model/",
    "@schema": "backend/schema/",
    "@service": "backend/service/"
  }
}
